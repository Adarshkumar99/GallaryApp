<div class="card">
    <div class="card-body">
        <%= form_with model: album do |form| %>
        <div class="field">
            <%= form.label :image %> <%= form.file_field :images, :multiple =>true, class: "form-control" %>
        </div>
        <br />
        <div>
            <%= form.submit "Add Image", class: "btn btn-secondary fw-bold" %>
        </div>
        <% end %>
    </div>
</div>